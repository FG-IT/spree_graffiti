<!--<div class="col-12 col-md-7 col-lg-7" data-hook="product_left_part">-->
  <% if images.size > 0 %>
    <div
      class="w-100 d-flex align-self-start justify-content-between product-details-images"
      data-hook="product_left_part_wrap"
    >
      <div class="d-none d-md-block product-details-thumbnails" data-hook="product_thumbnails">
        <%= render 'spree/products/graffiti/carousel/thumbnails',
                   images: images,
                   master_id: @presenter[:master_id],
                   dft_alt: @presenter[:name],
                   group_id: 'main-product-carousel',
                   id: 'productThumbnailsCarousel',
                   trigger_id: 'productCarousel',
                   per_page: 5
        %>
      </div>
      <div class="product-details-single" data-hook="product_images">
        <%= render 'spree/products/graffiti/carousel/single',
                   images: images,
                   master_id: @presenter[:master_id],
                   dft_alt: @presenter[:name],
                   group_id: 'main-product-carousel',
                   id: 'productCarousel',
                   trigger_id: 'productCarousel',
                   zoomed: false
        %>
      </div>
    </div>
  <% end %>
